{
  "extends": "../tsconfig.lib.json",
  "compilerOptions": {
    "outDir": "../../../dist",
    "lib": ["es2017", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "target": "es6"
  },
  "include": ["**/*"],
  "exclude": []
}
